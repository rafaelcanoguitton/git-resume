{
  "name": "git-resume",
  "displayName": "git resume",
  "description": "Resume work seemlessly and using git on the background!",
  "publisher": "rafxar",
  "repository": "https://github.com/rafaelcanoguitton/git-resume",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:git-resume-sidebar-view"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-resume.resume",
        "title": "Resume work on another computer"
      },
      {
        "command": "git-resume.hello",
        "title": "Hello World"
      },
      {
        "command": "git-resume.refresh",
        "title": "Refresh"
      },
      {
        "command": "git-resume.trpc",
        "title": "TRPC"
      },
      {
        "command": "git-resume.authenticate",
        "title": "Authenticate"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "git-resume-sidebar-view",
          "title": "Git Resume",
          "icon": "media/Git-Icon-White.svg"
        }
      ]
    },
    "views": {
      "git-resume-sidebar-view": [
        {
          "type": "webview",
          "id": "git-resume-sidebar",
          "name": "Git Resume",
          "icon": "media/Git-Icon-White.svg",
          "contextualTitle": "Git Resume"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "2",
    "@types/polka": "^0.5.4",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@vscode/test-electron": "^2.3.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@trpc/client": "^10.19.1",
    "@trpc/server": "^10.19.1",
    "abort-controller": "^3.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "node-fetch": "2",
    "polka": "^0.5.2",
    "superjson": "^1.12.2"
  },
  "icon": "media/git-resume-svg.png"
}
